      "@main/*": [
        "src/app/main/*"
      ],
      "@pages/*": [
        "src/app/pages/*"
      ],
      "@environment/*": [
        "src/environments/*"
      ],
      "@spartan-ng/ui-accordion-helm": [
        "./libs/ui/ui-accordion-helm/src/index.ts"
      ],
      "@spartan-ng/ui-alert-helm": [
        "./libs/ui/ui-alert-helm/src/index.ts"
