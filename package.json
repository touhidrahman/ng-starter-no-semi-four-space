{
  "name": "ng-starter-no-semi-four-space",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve",
    "build": "ng build",
    "build:alternative": "NODE_ENV=production node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e:open": "npx cypress open",
    "e2e:run": "npx cypress run",
    "spartan": "ng g @spartan-ng/cli:ui",
    "spartan-theme": "ng g @spartan-ng/cli:ui-theme",
    "lint": "npx biome lint ./src --apply",
    "lint:unsafe": "npx biome lint ./src --apply-unsafe",
    "prepare": "husky install .client/.husky",
    "format:ts": "npx biome format ./src ./libs --write",
    "format:html": "prettier --write 'src/**/*.{html,xml,scss,css,less}'",
    "format": "npm run format:ts && npm run format:html",
    "ng-update": "ng update @angular/core @angular/cli @angular/cdk @angular/material @schematics/angular --allow-dirty",
    "make-env-file": "tsc src/environments/make-env-file.ts && node src/environments/make-env-file.js && rm src/environments/make-env-file.js"
  },
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@ng-icons/core": "^26.3.0",
    "@ng-icons/radix-icons": "^26.3.0",
    "@ng-web-apis/common": "^3.0.6",
    "@spartan-ng/ui-accordion-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-alertdialog-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-avatar-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-checkbox-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-collapsible-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-command-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-core": "0.0.1-alpha.319",
    "@spartan-ng/ui-dialog-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-hovercard-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-menu-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-popover-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-progress-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-radiogroup-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-separator-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-sheet-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-switch-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-table-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-tabs-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-toggle-brain": "0.0.1-alpha.319",
    "@spartan-ng/ui-tooltip-brain": "0.0.1-alpha.319",
    "class-variance-authority": "^0.7.0",
    "clsx": "^1.2.1",
    "radash": "^11.0.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@biomejs/biome": "1.4.1",
    "@biomejs/cli-linux-x64": "1.4.1",
    "@schematics/angular": "^17.0.8",
    "@spartan-ng/cli": "^0.0.1-alpha.331",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.6",
    "autoprefixer": "^10.4.16",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "pretty-quick": "^3.1.3",
    "tailwind-merge": "^1.14.0",
    "tailwind-scrollbar": "^3.0.5",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-elevation": "^2.0.0",
    "typescript": "~5.2.2"
  },
  "lint-staged": {
    "src/**/*.{html,xml,scss,css,less}": "npx prettier --write",
    "src/**/*.{ts,js}": [
      "npx biome format --write",
      "npx biome lint --apply"
    ],
    "cypress/**/*.{ts,js}": [
      "npx biome format --write"
    ]
  }
}
